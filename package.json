{
  "name": "presentation-annotator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azu/presentation-annotator.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/azu/presentation-annotator",
  "license": "MIT",
  "files": [
    "src/",
    "lib/"
  ],
  "bugs": {
    "url": "https://github.com/azu/presentation-annotator/issues"
  },
  "version": "1.0.0",
  "description": "viewing presentation and annotate.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm-run-all clean --parallel watch:css server",
    "clean": "rimraf public/build && mkdirp public/build",
    "server": "NODE_ENV=development webpack-dev-server -d --hot --content-base ./public --open",
    "build:js": "NODE_ENV=production webpack --progress -p",
    "build:css": "postcss --config postcss.config.js src/index.css -o public/build/bundle.css",
    "build": "npm-run-all clean --parallel build:*",
    "watch:js": "webpack --debug --watch",
    "watch:css": "chokidar --initial 'src/**/*.css' -c 'npm run build:css'",
    "watch": "npm-run-all clean --parallel watch:css start",
    "prepublish": "npm run --if-present build",
    "test": "mocha"
  },
  "keywords": [
    "presentation",
    "pdf"
  ],
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "autoprefixer": "^7.0.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.6.0",
    "eslint-plugin-no-allow-react-context": "^1.0.1",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-suitcss-classnames": "^1.0.3",
    "gh-pages": "^0.12.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "npm-run-all": "^4.0.2",
    "postcss-calc": "^5.2.0",
    "postcss-cli": "^3.2.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-reporter": "^3.0.0",
    "power-assert": "^1.3.1",
    "rimraf": "^2.5.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.0.0"
  },
  "dependencies": {
    "almin": "^0.12.0",
    "almin-logger": "^4.0.3",
    "almin-reduce-store": "^1.0.1",
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.3",
    "file-saver": "^1.3.3",
    "jszip": "^3.1.3",
    "lodash.range": "^3.1.4",
    "map-like": "^1.0.1",
    "pdf.js-controller": "^1.0.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-pdf-js": "^1.0.35",
    "react-simple-file-input": "^1.0.0",
    "react-virtualized": "^9.7.5",
    "suitcss-base": "^3.0.0",
    "suitcss-classnames": "^1.1.0",
    "uuid": "^3.0.1"
  }
}
